		"url": "https://github.com/typescord/core/issues"
	},
	"dependencies": {
		"@discordjs/collection": "^0.1.6",
		"@typescord/famfor": "^0.1.2",
		"discord-api-types": "^0.18.1",
		"got": "^11.8.2",

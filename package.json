	"version": "0.0.0",
	"main": "build/index.js",
	"prettier": "@typescord/prettier-config",
	"scripts": {
		"prepack": "yarn build",
		"build": "rimraf build && tsc -p tsconfig.prod.json",
		"lint": "eslint src --ext ts",
